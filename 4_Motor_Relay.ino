void setup() {
  pinMode(11, OUTPUT);    //Set the pin 11 as an OUTPUT
}

void loop() {
  digitalWrite(11, HIGH); //Turn ON the pin 11
  delay(2000);            //Set a delay of 2 seconds
  digitalWrite(11, LOW);  //Turn OFF the pin 11
  delay(2000):            //Set a delay of 2 seconds
  }
